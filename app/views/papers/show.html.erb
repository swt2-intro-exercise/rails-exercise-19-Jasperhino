<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @paper.title %>
</p>

<p>
  <strong>Venue:</strong>
  <%= @paper.venue %>
</p>

<p>
  <strong>Year:</strong>
  <%= @paper.year %>
</p>

  <% @paper.authors.each do |author| %>
    <tr>
      <td><%= author.name %></td>
      <td><a href="<%= author.homepage %>"><%= author.homepage %></a></td>
    </tr>
  <% end %>

<%= link_to 'Edit', edit_paper_path(@paper) %> |
<%= link_to 'Back', papers_path %>
